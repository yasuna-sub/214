# サーバー設定
PORT=3000
NODE_ENV=development

# Google Cloud設定（共通）
VERTEX_PROJECT_ID=your-project-id
VERTEX_LOCATION=asia-northeast1
VERTEX_MODEL_NAME=gemini-1.0-pro
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=asia-northeast1
GOOGLE_APPLICATION_CREDENTIALS=your-service-account-key.json

# フロントエンド設定
VITE_BACKEND_URL=your-backend-url
VITE_GOOGLE_CLOUD_PROJECT_ID=${VERTEX_PROJECT_ID}
VITE_VERTEX_LOCATION=${VERTEX_LOCATION}
VITE_VERTEX_MODEL=${VERTEX_MODEL_NAME}
VITE_DEFAULT_AI_SERVICE=vertex

# CORS設定
CORS_ORIGINS=your-cors-origins

# 開発環境固有の設定
# 本番環境では.env.productionで上書き
DEV_DEBUG_MODE=true
DEV_LOG_LEVEL=debug 